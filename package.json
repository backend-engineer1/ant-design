{
  "name": "antd",
  "version": "0.0.1",
  "title": "Ant Design",
  "description": "下一个设计&前端框架",
  "homepage": "http://ant-design.github.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design/ant-design"
  },
  "bugs": {
    "url": "https://github.com/ant-design/ant-design/issues"
  },
  "dependencies": {
    "gregorian-calendar": "~3.0.0",
