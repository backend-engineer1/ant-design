    "rc-calendar": "~3.8.0",
    "rc-dialog": "~4.1.1",
    "rc-style": "git://github.com/ant-design/style.git",
    "rc-tooltip": "~1.2.0"
  },
  "devDependencies": {
    "babel-core": "~5.4.7",
    "babel-loader": "~5.1.3",
    "concurrently": "~0.1.0",
    "css-loader": "~0.13.1",
    "extract-text-webpack-plugin": "~0.8.1",
    "gh-pages-cli": "~0.2.0",
    "json-loader": "~0.5.1",
    "less": "~2.5.1",
    "less-loader": "~2.2.0",
