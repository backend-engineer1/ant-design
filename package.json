    "rc-collapse": "~1.2.3",
    "rc-dialog": "~4.5.0",
    "rc-dropdown": "~1.1.1",
    "rc-form-validation": "~2.4.7",
    "rc-input-number": "~2.0.1",
    "rc-menu": "~3.5.0",
    "rc-notification": "~1.1.0",
    "rc-pagination": "~1.1.0",
    "rc-progress": "~1.0.0",
    "rc-radio": "~2.0.0",
    "rc-select": "~4.5.2",
    "rc-slider": "~1.4.0",
    "rc-steps": "~1.1.4",
    "rc-switch": "~1.2.0",
    "rc-table": "~3.1.0",
    "rc-tabs": "~5.3.2",
