{
  "name": "rc-steps",
  "version": "1.0.0",
  "description": "steps ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-tabs"
  ],
  "main": "./lib/index",
  "homepage": "http://github.com/react-component/steps",
